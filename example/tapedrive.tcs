// Example schema testing custom type arrays and nested structs

package example;

enum Type {
  FLAT = 0;
  ROUND = 1;
  POINTED = 2;
}

struct Color {
  byte red;
  byte green;
  byte blue;
  byte alpha;
}

// A brush with a shape type and gradient colors
struct Brush {
  Type type;
  Color[] colors;
}

// A layer containing multiple brushes
struct Layer {
  string name;
  Brush[] brushes;
}

message Canvas {
  uint clientID = 1;
  uint width = 2;
  uint height = 3;
  Layer[] layers = 4;
}
